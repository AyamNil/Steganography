from PIL import Image
import numpy as np

def analyze_lsb(image_path):
    img = Image.open(image_path)
    arr = np.array(img)
    lsb = arr & 1  # Extract LSBs
    print(f"LSB analysis for {image_path}:")
    print(f"Unique LSB values (0/1): {np.unique(lsb, return_counts=True)}")
    print(f"LSB entropy (randomness): {np.mean(lsb)}")

analyze_lsb("output.png")
analyze_lsb("TEST.png")